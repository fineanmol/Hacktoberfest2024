def greatest_number():
    a=list(map(int,input("Input the 5 numbers with spaces - ").split(' ')))
    if len(a)==5:
        flag=a[0]
        for i in a[1:]:
            if i>=flag:
                flag=i
            else:
                continue
        print("The greatest number is",flag)
    else:
        print('You were supposed to enter 5 numbers.')
        
        
greatest_number()
