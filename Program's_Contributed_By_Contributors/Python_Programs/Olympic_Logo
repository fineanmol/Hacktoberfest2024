import turtle
t = turtle.Turtle()
t.pensize(6)
t.goto(0,0)

firstcolors=['blue', 'black', 'red']
for i in range(3):
    t.penup()
    t.pencolor(firstcolors[i])
    t.goto(i*130,0)
    t.pendown()
    t.circle(60)
secondcolors=['','yellow','', 'green']
for i in range(1,4):
    t.penup()
    t.pencolor(secondcolors[i])
    t.goto(i*65, -66)
    t.pendown()
    t.circle(60)
