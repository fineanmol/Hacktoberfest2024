class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        int start = -1;
        int ans = 0;
        vector<int>d(256,-1);
        for(int i = 0;i<s.length();i++){
            if(d[s[i]]>start) start = d[s[i]];
            d[s[i]] = i;
            ans = max(ans,i-start);
        }
        return ans;
    }
};
