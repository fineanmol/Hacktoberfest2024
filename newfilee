# Displaying a string  
print("Hello, World!")  
  
# Displaying multiple values  
name = "Aman"  
age = 21  
print("Name:", name, "Age:", age)  
  
# Printing variables and literals  
x = 5  
y = 7  
print("x =", x, "y =", y, "Sum =", x + y)  
  
# Printing with formatting  
percentage = 85.75  
print("Score: {:.2f}%".format(percentage)) 
