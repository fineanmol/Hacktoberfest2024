#include<iostream>
#include<bits/stdc++.h>
using namespace std;
void unionSortedArray(vector<int>a,vector<int>b){
int i=0;
vector<int> res;
int j = 0;
while (i<a.size()&& j<b.size())
{
    if(i>0 && a[i]==a[i-1]){
        i++;
        continue;
    }
    if (j > 0 && b[j] == b[j - 1])
    {
        j++;
        continue;
    }
if(a[i]<b[j]){
res.push_back(a[i]);
i++;
}
else if(a[i]>b[j]){
res.push_back(b[i]);
j++;
}
else{
    res.push_back(a[i]);
    i++;j++;
}
}
if(i<a.size()){
    while (i<a.size())
    {
        if (i > 0 && a[i] == a[i - 1])
        {
            i++;
            continue;
        }
        res.push_back(a[i]);
        i++;
    }
    
   
}
if(j<b.size()){
    while (j<b.size())
    {
        if (j > 0 && b[j] == b[j - 1])
        {
        j++;
            continue;
        }
        res.push_back(b[j]);
        j++;
    }
    
}
for (int i = 0; i < res.size(); i++)
{
    cout<<res[i]<<" ";
}

}
int main()
{
    vector<int>a={2,3,3,3,4,4};
    vector<int>b={4,4};
    unionSortedArray(a,b);
return 0;
}
